{
  "name": "testing-react",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test:unit": "nyc mocha --require @babel/register --require ignore-styles --require ./test/helpers.js --require ./test/dom.js 'src/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:coverage": "nyc report --reporter=text-lcov >> ./coverage/lcov-unit.info",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot -- --watch",
    "test:cypress": "start-server-and-test start http://localhost:8080 cypress",
    "cypress": "cypress run",
    "coveralls": "lcov-result-merger 'coverage/lcov*.info' | node node_modules/.bin/coveralls"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "cypress": "^3.1.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "lcov-result-merger": "^3.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.6.3",
    "sinon": "^7.1.1",
    "start-server-and-test": "^1.7.10",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "build",
      "test"
    ],
    "report-dir": "./coverage"
  }
}
